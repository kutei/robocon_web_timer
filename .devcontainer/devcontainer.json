{
    "name": "react-web",
    "build": {
        "dockerfile": "Dockerfile",
    },
    "workspaceFolder": "/app",
    "workspaceMount": "source=${localWorkspaceFolder},target=/app,type=bind,consistency=cached",
    "remoteUser": "devuser",
    "updateRemoteUserUID": true,
    "customizations": {
        "vscode": {
            "extensions": [
                "mhutchie.git-graph",
                "ms-azuretools.vscode-docker",
            ]
        }
    }
}
